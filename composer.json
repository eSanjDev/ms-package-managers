{
    "name": "esanj/managers",
    "description": "The Manager package is responsible for handling OAuth2-based authentication and managing admin-level permissions.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Esanj Team"
        }
    ],
    "autoload": {
        "psr-4": {
            "Esanj\\Manager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esanj\\Manager\\Providers\\ManagerServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "firebase/php-jwt": "^6.11",
        "esanj/auth-bridge": "^0.1.4|^0.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
